# Cheesboard improved version with out square colors nested for loop demonstraion
# @author: Akhtar Jalbani

import turtle
turtle = turtle.Turtle()
turtle.speed(10)
pos = 0
for row in range(8):
  for col in range(8):
    for i in range(4):
      turtle.forward(100)
      turtle.left(90)
    turtle.forward(100)
  turtle.penup()
  pos = pos+100
  turtle.goto(0,pos)
  turtle.pendown()
